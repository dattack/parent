<project xmlns="http://maven.apache.org/DECORATION/1.6.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">

	<bannerLeft>
		<name>dattack Team</name>
		<src>http://dattack.com/img/logo.png</src>
		<href>http://www.dattack.com</href>
	</bannerLeft>

	<version position="right"/>
	<publishDate position="right"/>

	<poweredBy>
		<logo name="dattack" href="http://www.dattack.com" img="http://dattack.com/img/logo.png"/>
	</poweredBy>

	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.7</version>
	</skin>

	<custom>
		<fluidoSkin>
			<topBarEnabled>true</topBarEnabled>
			<sideBarEnabled>true</sideBarEnabled>
			<googleSearch />
			<sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
			<gitHub>
				<projectId>dattack/parent</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>black</ribbonColor>
			</gitHub>
			<twitter>
				<user>dattackteam</user>
				<showUser>true</showUser>
				<showFollowers>true</showFollowers>
			</twitter>
		</fluidoSkin>
	</custom>

	<body>
		<menu ref="reports" />
	</body>
</project>
